<?php
// Fungsi untuk mengecek bilangan
function cekBilangan($bilangan) {
    if ($bilangan > 10) {
        echo "Bilangan " . $bilangan . " adalah bilangan positif.<br>";
    } elseif ($bilangan < -5) {
        echo "Bilangan " . $bilangan . " adalah bilangan negatif.<br>";
    } else {
        echo "Bilangan tersebut adalah 0.<br>";
    }
}
?>
